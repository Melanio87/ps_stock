	<queryString>
		<![CDATA[select ven_codigo,ven_fecha,ven_hora,ven_codven,acc_nombre,itv_codpro,pro_descri,
itv_canti,itv_precio,ven_total,pro_pre1,(itv_canti * pro_pre1) as ven_cosTot,ven_cliente,case when cli_nombre is null then 'Ocacional' else cli_nombre end,case when ven_tipo=0 then 'CONTADO' else 'CREDITO' end as ven_tipo,ven_obs,avt_fecha,avt_hora,avt_codusu,avt_numven,avt_motivo,
case when ven_numfac ='null' then '' else ven_numfac end as ven_numfac
from ventas left join clientes cli on cli.cli_codigo = ventas.ven_cliente,item_ventas,productos,acceso,anuvent
where pro_codigo=itv_codpro and ven_codigo=itv_codven
and acc_id=ven_codven and ven_fecha >= $P{fecI} and ven_fecha <= $P{fecF} and ven_codigo=itv_codven and ven_situ=0 and avt_numven=ven_codigo order by ven_codigo]]>
